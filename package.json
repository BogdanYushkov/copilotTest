{
  "name": "gptTest",
  "version": "1.0.0",
  "scripts": {
    "pretest": "rimraf allure-results allure-report playwright-report test-results",
    "test": "playwright test",
    "test:ci": "cross-env CI=true playwright test",
    "report:allure": "allure generate allure-results -o allure-report --clean",
    "report:serve": "allure serve allure-results"
  },
  "devDependencies": {
    "@playwright/test": "latest",
    "allure-playwright": "latest",
    "allure-commandline": "latest",
    "cross-env": "latest",
    "rimraf": "latest"
  }
}
